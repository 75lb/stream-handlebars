{
  "name": "stream-handlebars",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.1.3",
  "description": "Extends handlebars with a streaming interface for .compile()",
  "repository": "https://github.com/75lb/stream-handlebars.git",
  "main": "./lib/stream-handlebars.js",
  "bin": {
    "stream-handlebars": "bin/cli.js"
  },
  "keywords": [
    "handlebars",
    "stream",
    "compile"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js |tap-spec",
    "lint": "jshint lib/*.js bin/*.js test/*.js; echo",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs -l js lib/*.js > README.md; echo"
  },
  "dependencies": {
    "handlebars": "^3.0.0",
    "object-tools": "^1.2.1"
  },
  "devDependencies": {
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  },
  "license": "MIT"
}
